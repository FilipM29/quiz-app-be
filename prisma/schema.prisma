generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Quiz {
    id        String    @id @default(cuid())
    name      String
    createdAt DateTime  @default(now()) @map("created_at") @db.Timestamptz
    updatedAt DateTime  @updatedAt @map("updated_at") @db.Timestamptz

    @@map("quiz")
}